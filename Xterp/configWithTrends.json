{
	"data": {
		"filename": "data/joined.csv",
		"columns": [
			"Close",
			"Open",
			"High",
			"Low",
			"Volume",
			"Diversified REITs",
			"Health Care REITs",
			"Hotel",
			"Industrial REITs",
			"Real Estate Development",
			"Residential REITs",
			"Retail REITs",
			"ICSA",
			"UNRATE"
		],
		"sequence_length": 14,
		"train_test_split": 0.85,
		"normalise": true
	},
	"training": {
		"epochs": 12,
		"batch_size": 32
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models",
		"layers": [
			{
				"type": "lstm",
				"neurons": 200,
				"input_timesteps": 13,
				"input_dim": 14,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.1
			},
			{
				"type": "lstm",
				"neurons": 200,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 200,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	}
}
