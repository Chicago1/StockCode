{
	"data": {
		"filename": "data/joined.csv",
		"columns": [
			"Close",
			"Open",
			"High",
			"Low",
			"Volume",
			"Economic Bubble",
			"Advertising",
			"Gross Domestic Product",
			"Interactive Media",
			"Broadcasting",
			"Cable",
			"Alternative Carriers",
			"Technical Analysis",
			"Federal Reserve",
			"Bar Chart",
			"Diversification",
			"Analyst Ratings",
			"Return on Equity",
			"Accumulation",
			"Dead Cat Bounce",
			"Movies"
		],
		"sequence_length": 50,
		"train_test_split": 0.85,
		"normalise": true
	},
	"training": {
		"epochs": 50,
		"batch_size": 32
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models",
		"layers": [
			{
				"type": "lstm",
				"neurons": 200,
				"input_timesteps": 49,
				"input_dim": 21,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.1
			},
			{
				"type": "lstm",
				"neurons": 200,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 200,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	}
}
