{
	"data": {
		"filename": "data/joined_yovery.csv",
		"columns": [
			"Close",
			"Open",
			"High",
			"Low",
			"Volume",
			"Diversified REITs",
			"Health Care REITs",
			"Hotel",
			"Industrial REITs",
			"Real Estate Development",
			"Residential REITs",
			"Retail REITs"
		],
		"sequence_length": 7,
		"train_test_split": 0.95,
		"normalise": true
	},
	"training": {
		"epochs": 200,
		"batch_size": 32
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models",
		"layers": [
			{
				"type": "lstm",
				"neurons": 300,
				"input_timesteps": 6,
				"input_dim": 12,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "lstm",
				"neurons": 300,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 300,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	}
}
